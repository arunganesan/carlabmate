<?xml version="1.0" encoding="utf-8"?>
<manifest 
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  package="edu.umich.carlab.template"
  >

          <application
            tools:replace="android:icon, android:roundIcon"
            android:name="edu.umich.carlab.template.MyApplication"
            android:allowBackup="true"
            android:icon="@mipmap/space_icon"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/space_icon"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="edu.umich.carlab.template.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="edu.umich.carlab.template.ManualTrigger" />
    </application>
</manifest>
